{
  "name": "openvpn installer",
  "description": "OpenVPN installer JSON",
  "version": "0.0.1",
  "url": "https://github.com/SamuelMarks/deploy-sh",
  "license": "Apache-2.0 OR MIT OR CC0",
  "scripts_root": "${SCRIPT_ROOT_DIR}",
  "wwwroot": [],
  "dependencies": {
    "required": {
      "toolchains": [
        {
          "name": "CC",
          "version": "*",
          "env": "CC_INSTALL_DIR"
        }
      ],
      "servers": [
        {
          "name": "openvpn",
          "daemon": {
            "os_native": true
          }
        }
      ]
    },
    "optional": {}
  },
  "log_server": {
    "optional": true
  }
}